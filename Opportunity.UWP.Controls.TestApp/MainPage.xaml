<Page x:Class="Opportunity.UWP.Controls.TestApp.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:local="using:Opportunity.UWP.Controls.TestApp"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      mc:Ignorable="d"
      xmlns:controls="using:Opportunity.UWP.Controls"
      Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
  <Page.Resources>
    <controls:DataTemplateSelector x:Name="kk">
      <controls:DataTemplateKeyValuePair KeyType="Page" />
      <controls:DataTemplateKeyValuePair KeyType="controls:DataTemplateKeyValuePair" />
      <controls:DataTemplateKeyValuePair KeyType="local:MainPage" />
      <controls:DataTemplateKeyValuePair Key="Opportunity.UWP.Controls.TestApp.XX" />
    </controls:DataTemplateSelector>
  </Page.Resources>
  <Grid>

  </Grid>
</Page>
